{
	"name": "gitmoon",
	"version": "0.0.1",
    "dependencies": {        
        "async": "~0.1.22",
        "connect": "~2.4.2",
        "date-utils": "~1.2.11",
        "express": "~2.5.9",
        "express-resource": "https://github.com/yaronn/express-resource/tarball/master",
        "express-streamline": "~0.1.3",               
        "memcached": "~0.0.11",
        "mongodb": "~0.9.9-7",
        "neo4j": "~0.2.12",
        "request": "~2.9.202",
        "streamline": "~0.4.0",
        "temp": "~0.4.0",        
        "wd": "~0.0.20",
        "xregexp": "~2.0.0",
        "coffee-script": "1.3.3"      
    },
    "devDependencies": {
    	"assert": "~0.4.9"
    },
    "engines": {
        "node": ">= 0.6"
    },
    "scripts": {
        "build": "_coffee -c server._coffee common/ service/; _coffee ./common/crunch._coffee",        
        "clean": "rm server.js service/*.js common/*.js ./site/crunch.js ./site/crunch.css",
        "run-dev": "_coffee ./common/crunch._coffee; supervisor --exec '_coffee' server._coffee",
        "run-prod": "forever -w run.js",
        "test": "mocha -t 40000 --compilers coffee:coffee-script,_coffee:streamline/register ./test/ui._coffee"
    },
    "repository" : {
        "type" : "git",
        "url" : "https://github.com/yaronn/gitmoon.git"
    }
}